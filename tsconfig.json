{
  "compilerOptions": {
    "target": "ES2022",
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "rootDir": "src", 
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "allowImportingTsExtensions": true
    
  },
  "include": ["src"]
}
